# Build configuration
EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = NO
EXTRACT_STATIC         = YES
EXTRACT_LOCAL_CLASSES  = YES
EXTRACT_LOCAL_METHODS  = NO
EXTRACT_ANON_NSPACES   = NO

# Input configuration
INPUT                  = .
RECURSIVE              = YES
FILE_PATTERNS          = *.c *.cpp *.cxx *.cc *.C *.h *.hpp *.hxx *.hh *.H \
                          *.py *.pyx *.pyi *.java *.php *.js *.jsx *.ts *.tsx \
                          *.cs *.go *.rs *.rb *.pl *.sh
EXCLUDE_PATTERNS       = */.*/* */build/* */dist/* */target/* */bin/* */obj/* \
                          */node_modules/* */__pycache__/* *.tmp *.bak
EXCLUDE_SYMBOLS        = 

# Language optimization
OPTIMIZE_OUTPUT_FOR_C  = NO
OPTIMIZE_OUTPUT_JAVA   = NO
OPTIMIZE_FOR_FORTRAN   = NO
OPTIMIZE_OUTPUT_VHDL   = NO

# Output formats
GENERATE_HTML          = YES
GENERATE_LATEX         = NO
GENERATE_RTF           = NO
GENERATE_MAN           = NO
GENERATE_XML           = YES
GENERATE_DOCBOOK       = NO

# HTML configuration
HTML_OUTPUT            = html
HTML_FILE_EXTENSION    = .html
HTML_COLORSTYLE        = AUTO_LIGHT
HTML_DYNAMIC_MENUS     = YES
HTML_DYNAMIC_SECTIONS  = NO
HTML_INDEX_NUM_ENTRIES = 100
