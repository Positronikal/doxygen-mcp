# Standard Doxyfile Template
# Generated by Doxygen MCP Server
# Template: Standard - Balanced feature set for most projects

# Project information
PROJECT_NAME           = "My Project"
PROJECT_NUMBER         = 
PROJECT_BRIEF          = 
PROJECT_LOGO           = 
